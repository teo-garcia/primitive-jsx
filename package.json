{
  "version": "1.0.2",
  "name": "primitivex",
  "author": "teo-garcia",
  "license": "MIT",
  "isLibrary": true,
  "source": "src/index.tsx",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">= 15"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "dev:lib": "parcel watch",
    "build:lib": "parcel build",
    "dev:sandbox": "cd sandbox && parcel -p 3000",
    "build:sandbox": "cd sandbox && parcel",
    "test:lib": "jest --passWithNoTests",
    "lint:lib": "tsdx lint",
    "prepare:lib": "tsdx build",
    "size:lib": "size-limit",
    "analyze:lib": "size-limit --why",
    "dev:sb": "start-storybook -p 6006",
    "build:sb": "build-storybook"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "parcel": "^2.6.2",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
